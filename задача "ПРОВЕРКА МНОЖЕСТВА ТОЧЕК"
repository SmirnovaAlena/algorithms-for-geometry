#include<iostream>
#include<vector>
#include<string>
#include<algorithm>
using namespace std;
string func(pair<int, int> p, vector<pair<int, int>> v) {
	int x= p.first;
	int y = p.second;
	int n = v.size();
	bool flag = false;
	for (int i = 0; i < n; i++) {
		int x1 = v[i].first;
		int y1 = v[i].second;
		int x2 = v[(i+1)%n].first;
		int y2 = v[(i + 1) % n].second;
		if (min(x1, x2) <= x && x <= max(x1, x2) && min(y1, y2) <= y && y <= max(y1, y2) && abs((x2 - x1) * (y - y1) - (y2 - y1) * (x - x1)) < 1e-9) {
			return "BORDER";
		}
		if ((y1 > y) != (y2 > y)) {
			int xc = x1 + (x2 - x1) * (y - y1) / (y2 - y1);
			if (xc > x) {
				flag = not(flag);
			}
		}
	}
	if (flag) {
		return "INSIDE";
	}
	else {
		return "OUTSIDE";
	}
}
int main() {
	int n;
	cin >> n;
	vector<pair<int, int>> v;
	for (int i = 0; i < n; i++) {
		int x, y;
		cin >> x >> y;
		v.push_back({ x,y });

	}
	int m;
	cin >> m;
	for (int i = 0; i < m; i++) {
		int x1, y1;
		cin >> x1 >> y1;
		cout << func({ x1,y1 }, v) << endl;
	}
	
	return 0;
}
